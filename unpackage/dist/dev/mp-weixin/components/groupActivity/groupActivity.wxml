<view class="groupActivity data-v-bfa069ce"><block wx:for="{{activityList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="orderItem data-v-bfa069ce"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['activityList','',index]]]]]]]}}" class="titleBox data-v-bfa069ce" bindtap="__e"><view class="text data-v-bfa069ce">{{''+item.title+''}}</view><van-icon class="arrowIcon data-v-bfa069ce" vue-id="{{'1-'+index}}" name="arrow" size="28rpx" color="#3c3731" data-com-type="wx" bind:__l="__l"></van-icon></view><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['activityList','',index]]]]]]]}}" class="content data-v-bfa069ce" bindtap="__e"><view class="imgBox data-v-bfa069ce"><image style="width:100%;height:100%;" src="{{item.venueImage}}" mode class="data-v-bfa069ce"></image></view><view class="detail data-v-bfa069ce"><view class="clubName data-v-bfa069ce">{{"地点："+item.venueName}}</view><view class="date data-v-bfa069ce">{{"日期："+item.time}}</view><view class="time data-v-bfa069ce">{{"时间："+item.timeStart+"-"+item.timeEnd}}</view></view></view><view class="btnBox data-v-bfa069ce"><view class="people data-v-bfa069ce"><text hidden="{{!(showPeople)}}" class="data-v-bfa069ce">{{"共："+item.enrolledTotal+"人报名"}}</text></view><view class="left data-v-bfa069ce"><view data-event-opts="{{[['tap',[['clickBtn1',['$0'],[[['activityList','',index]]]]]]]}}" hidden="{{!(showBtn)}}" class="btn btn1 data-v-bfa069ce" bindtap="__e">{{btn1Text}}</view><view data-event-opts="{{[['tap',[['clickBtn2',['$0'],[[['activityList','',index]]]]]]]}}" class="btn btn2 data-v-bfa069ce" bindtap="__e">{{btn2Text}}</view></view></view></view></block><view hidden="{{!(isShowNoData)}}" class="noData data-v-bfa069ce"></view></view>